<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mybatis.mapper.TimetableMapper">
	
	
	<!-- 학생번호,시간표이름으로 검색 -->
	<select id="selectOne" parameterType="hashmap" resultType="com.jhta.finalproject.timetablevo.TimetableVo">
		select * from TIMETABLE where s_num=#{s_num} and m_num=#{m_num}
	</select>
	
	<insert id="insert" parameterType="com.jhta.finalproject.timetablevo.TimetableVo">
		insert into TIMETABLE 
		values(TIMETABLE_seq.NEXTVAL,#{s_num},#{m_num},#{t_name})
	</insert>
	<update id="update" parameterType="com.jhta.finalproject.timetablevo.TimetableVo">
		update TIMETABLE 
		set s_num=#{s_num} where t_num=#{t_num} and t_name=#{t_name}
	</update>
	<delete id="delete" parameterType="int">
		delete from TIMETABLE where t_num=#{t_num}
	</delete>
</mapper>