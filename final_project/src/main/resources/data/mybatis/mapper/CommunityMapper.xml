<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="data.mybatis.mapper.CommunityMapper">
 	<insert id="insertcm" parameterType="com.jhta.finalproject.vo.CommunityVo">
		insert into community
    	(
   		 cu_num, cu_name , cu_intro , cu_category , cu_coverimg , m_num
   		 )
    	values
    
   		 (
   		 community_seq.nextval,#{cu_name},#{cu_intro},#{cu_category},#{cu_coverimg},#{m_num}
  		)
	</insert>
	<select id="selectUser" parameterType="String" resultType="com.jhta.finalproject.vo.MemberVo">
     	select * from member where m_id=#{m_id}
     </select>
     
     <select id="selectAll" resultType="com.jhta.finalproject.vo.CommunityVo">
		select * from community where cu_status='1'
	</select>
 </mapper>
 
 
 

 
 
 
 
 
 
 
 