<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="data.mybatis.mapper.MemberMapper">
    <insert id="insertmember" parameterType="com.jhta.finalproject.vo.MemberVo"> 
    insert into member values(member_seq.nextval,#{m_id},#{m_pwd},#{m_name},#{m_phone},#{m_email},#{m_address},#{m_nickname},#{m_birth},#{m_dept},#{m_univnum},1,1)
    	
    </insert>
    <insert id="authInsert" parameterType="hashmap">
    	insert into authorities values(#{m_num},#{authority})
    </insert>
    
    
 </mapper>
 
 
 

 
 
 
 
 
 
 
 